{
  "name": "nuxt-ganalytics",
  "version": "1.0.2",
  "description": "Integration for Google Analytics 4 and Google Tag Manager in Nuxt",
  "repository": "https://github.com/Zadigo/nuxt-ganalytics",
  "author": {
    "name": "John Pendenque",
    "email": "pendenquejohn@gmail.com"
  },
  "readme": "README.md",
  "license": "MIT",
  "type": "module",
  "keywords": ["google analytics", "ga4", "google tag manager", "gtm", "analytics"],
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types.d.mts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "npm run dev:prepare && nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "peerDependencies": {
    "nuxt": "^3.0.0 || ^4.0.0",
    "@vueuse/core": "^13.0.0"
  },
  "dependencies": {
    "@gtm-support/vue-gtm": "^3.1.0",
    "@vueuse/core": "^13.4.0",
    "@nuxt/kit": "^4.0.2",
    "defu": "^6.1.4",
    "ufo": "^1.6.1"
  },
  "devDependencies": {
    "@nuxt/devtools": "^2.5.0",
    "@nuxt/eslint": "1.4.1",
    "@nuxt/eslint-config": "^1.4.1",
    "@nuxt/fonts": "0.11.4",
    "@nuxt/module-builder": "^1.0.1",
    "@nuxt/schema": "^4.0.2",
    "@nuxt/scripts": "0.11.8",
    "@nuxt/test-utils": "^3.19.1",
    "@types/node": "latest",
    "@unhead/vue": "^2.0.10",
    "@nuxt/devtools-kit": "^2.5.0",
    "@vitest/coverage-v8": "3.2.4",
    "@vue/test-utils": "^2.4.6",
    "@vueuse/nuxt": "13.4.0",
    "@tailwindcss/vite": "^4.1.10",
    "@nuxt/ui": "^3.1.3",
    "tailwindcss": "^4.1.10",
    "changelogen": "^0.6.1",
    "eslint": "^9.29.0",
    "happy-dom": "^18.0.1",
    "nuxt": "^4.0.2",
    "typescript": "~5.8.3",
    "vitest": "^3.2.3",
    "vue": "^3.5.16",
    "vue-tsc": "^3.0.4"
  },
  "packageManager": "pnpm@10.12.1+sha512.f0dda8580f0ee9481c5c79a1d927b9164f2c478e90992ad268bbb2465a736984391d6333d2c327913578b2804af33474ca554ba29c04a8b13060a717675ae3ac"
}
